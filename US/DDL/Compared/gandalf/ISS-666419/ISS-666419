CREATE TABLE `postgres_instance_owner_info` ( `id` int NOT NULL AUTO_INCREMENT, `instance_name` varchar(50) DEFAULT NULL, `instance_ip` varchar(100) DEFAULT NULL, `instance_role` varchar(20) DEFAULT NULL, `owner_email` varchar(50) DEFAULT NULL, `approval_type` varchar(25) DEFAULT NULL, `secondary_email` varchar(130) DEFAULT NULL, `secondaryapprovalType` varchar(30) DEFAULT NULL, `third_email` varchar(130) DEFAULT 'NA', `thirdapprovalType` varchar(30) DEFAULT 'NA', `aws_account_name` varchar(20) DEFAULT 'RSPL', `rspl_admin_email` varchar(50) DEFAULT NULL, PRIMARY KEY (`id`), UNIQUE KEY `owner_name` (`instance_ip`,`owner_email`,`third_email`) ) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;
